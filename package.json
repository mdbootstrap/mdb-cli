{
  "name": "mdb-cli",
  "version": "2.3.0",
  "description": "Command Line Interface for MDB",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mdbootstrap/mdb-cli.git"
  },
  "scripts": {
    "test": "mocha --exit --recursive --file test/index.js",
    "test:coverage": "nyc --reporter=text mocha --exit --recursive --file test/index.js",
    "compile": "./node_modules/pkg/lib-es5/bin.js -c package.json dist/index.js --targets node6",
    "lint": "./node_modules/.bin/eslint .",
    "lintfix": "./node_modules/.bin/eslint . --fix"
  },
  "bin": {
    "mdb": "dist/index.js"
  },
  "author": "MDBootstrap.com",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "chai": "^4.2.0",
    "eslint": "^6.1.0",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0",
    "mocha": "^6.1.1",
    "nyc": "^13.3.0",
    "pkg": "^4.3.5",
    "sinon": "^7.3.1",
    "sinon-chai": "^3.5.0"
  },
  "dependencies": {
    "archiver": "^3.0.0",
    "atob": "^2.1.2",
    "babel-polyfill": "^6.26.0",
    "btoa": "^1.2.1",
    "console.table": "^0.10.0",
    "dotenv": "^8.0.0",
    "fs-extra": "^8.0.1",
    "inquirer": "^6.2.1",
    "open": "^7.3.0",
    "ora": "^3.4.0",
    "progress": "^2.0.3",
    "unzipper": "^0.9.7"
  },
  "pkg": {
    "assets": [
      "dist/commands/*.js",
      "dist/config/*.js",
      "dist/helpers/*.js",
      "dist/models/*.js",
      "dist/utils/*.js"
    ]
  },
  "nyc": {
    "exclude": [
      "test/",
      "config/"
    ]
  }
}
